<script>
function showAd() {
   $('.header-fixed, .main-uninav-header').css("margin-top", function() {
       return $('.news-leader-wrap').innerHeight();
   });

   $('.news-main-wrapper').css("top", function() {
       return $('.news-leader-wrap').innerHeight();
   });
   var myVar = setTimeout(function() {
       $('.header-fixed, .main-uninav-header').css("margin-top", 0);
       $('.news-main-wrapper').css("top", "0px");
       $('.news-leader-wrap,.news-leader-bottom').fadeOut();
   }, 20000);
}

$(document).ready(function() {
   googletag.cmd.push(function() {
       var SLBmapping = googletag.sizeMapping()
           .addSize([970, 0], [
               [970, 90],
               [728, 90]
           ])
           .addSize([728, 0], [728, 90])
           .addSize([0, 0], [
               [320, 50],
               [320, 100]
           ])
           .build();
       var SLBslot = googletag.defineSlot('/2744311/ABSNews_SuperLeaderboard', [
           [320, 50],
           [320, 100],
           [728, 90],
           [970, 90]
       ], 'div-gpt-ad-SLB').defineSizeMapping(SLBmapping).addService(googletag.pubads());
       googletag.enableServices();
       googletag.display('div-gpt-ad-SLB');
       googletag.pubads().refresh([SLBslot]);
       googletag.pubads().addEventListener('slotOnload', function(event) {
           if (event.slot.getSlotId().getId() == '/2744311/ABSNews_SuperLeaderboard_0') {
               setTimeout(showAd, 1000)
           }
       })
       $('.close').on('click', function(e) {
           e.preventDefault();
           googletag.destroySlots([SLBslot]); // we added this to destroy slot not just hide.
           $('.news-leader-wrap,.news-leader-bottom').fadeOut();
           $('.header-fixed, .main-uninav-header').css("margin-top", 0);
           $('.news-main-wrapper').css("top", "0px");
       });
   });
});
$(function() {
   //caches a jQuery object containing the header element
   var header = $(".news-leader-wrap");
   $(window).scroll(function() {
       if ($('.news-leader-wrap').height() < 350) {
           var scroll = $(window).scrollTop();
           if (scroll >= 100) {
               header.removeClass('news-leader-wrap').addClass("news-leader-bottom");
               $('.header-fixed, .main-uninav-header').css("margin-top", 0);
               $('.news-main-wrapper').css("top", "0px");
           } else {
               $('.header-fixed, .main-uninav-header').css("margin-top", function() {
                   $('.news-main-wrapper').css("margin-top", 30);
                   return $('.news-leader-wrap').innerHeight();
               });
           }
       }
   });
});
</script>
<script>
	$(document).ready(function () {
		var adUnit1= 'div-gpt-ad-MRECUL';
		//var adUnit2= 'div-gpt-ad-MRECLR';
		function getoffset(id) {
			 return $('#' + id).offset().top;
		}
		function lazyLoaded(id, bolean) {
			 var loaded= (bolean) ? $('#' + id).attr('loaded', bolean) : $('#' + id).attr('loaded');
			 return loaded;
		}
		function lazyLoadAd(div_id, adunit, adformat) {
		 if ($(window).scrollTop() + $(window).height() >= getoffset(div_id)) {
				if (!lazyLoaded(div_id)) {
				 lazyLoaded(div_id, true);
				 console.log('loading ad unit ' + adunit + ' for ' + div_id);
				 var size, mapping;
				 if (adformat== 'Banner') {
						size= [[300, 250], [320, 50], [320, 100]];
						mapping= googletag.sizeMapping()
						.addSize([728, 0], [[320, 50], [320, 100]])
						.addSize([0, 0], [300, 250])
						.build();
				 } else if (adformat== 'MREC') {
						size= [[300,250]];
						mapping= googletag.sizeMapping()
						.addSize([0, 0], [[300,250]])
						.build();
				 }
				 var gad= googletag.defineSlot(adunit, size, div_id).defineSizeMapping(mapping).addService(googletag.pubads());
				 googletag.enableServices();
				 googletag.display(div_id);
				 googletag.pubads().refresh([gad]);
				}
		 }
		};

		$(window).scroll(function () {
			 lazyLoadAd(adUnit1, '/2744311/ABSNews_Banner', 'MREC');
			 //lazyLoadAd(adUnit2, '/2744311/ABSNews_MRECLR', 'MREC');
		});      
	});

