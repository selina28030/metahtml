         var slbUnfilled = 'noValue', slbViewable = 'noValue';
         googletag.cmd.push(function() {
            var SLBmapping = googletag.sizeMapping()
               .addSize([970, 0], [[970, 90], [970,250],[728,90]])
               .addSize([728, 0], [728, 90])
               .addSize([0, 0], [[320, 50],[320,100]])
               .build();
            var SLBslot = googletag.defineSlot('/2744311/ABSNews_SuperLeaderboard', [[320, 50], [320,100], [728, 90], [970, 90]], 'div-gpt-ad-SLB').defineSizeMapping(SLBmapping).addService(googletag.pubads());
            googletag.enableServices();
            googletag.display('div-gpt-ad-SLB');
            googletag.pubads().refresh([SLBslot]);
            googletag.pubads().addEventListener('slotRenderEnded', function(event) {
               if (event.slot.getSlotElementId() == "div-gpt-ad-SLB") {
                  slbUnfilled = event.isEmpty;
               }
            });
            googletag.pubads().addEventListener('impressionViewable', function(event) {
               if (event.slot.getSlotElementId() == "div-gpt-ad-SLB") {
                  slbViewable = true;
               }
            });
         });
