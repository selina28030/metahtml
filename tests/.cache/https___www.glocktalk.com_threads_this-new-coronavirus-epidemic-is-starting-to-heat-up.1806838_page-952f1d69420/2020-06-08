      googletag.cmd.push(function() { deployads.push(function() {deployads.gpt.display('div-gpt-ad-content2'); });});
      googletag.defineSlot('/1030735/glocktalk.com/thread_content3', [[300, 250], [600, 400]], 'div-gpt-ad-content3').addService(googletag.pubads());
      googletag.cmd.push(function() { deployads.push(function() {deployads.gpt.display('div-gpt-ad-content3'); });});

            //DFP defineslot and display code goes here
            firedFor25pct = true;
            console.log('The 25% scroll threshold lazy load ads should be firing now!');        
       }

       //DFP ads that should fire when scroll gets to 25% of the document
       
       if ($(window).scrollTop()>= (($(document).height() / 0.90) - 300) && !firedFor50pct)
       { 
           //DFP defineslot and display code goes here
           firedFor50pct = true; 
           console.log('The 50% scroll threshold lazy load ads should be firing now!');
        }

        //DFP ads that should fire when scroll gets to 50% of the document
        
        if ($(window).scrollTop()>= (($(document).height() / 1.35) - 300) && !firedFor75pct)
        { 
    var mappingbottomleader = googletag.sizeMapping().

      addSize([990, 300], [[728, 90], [970, 250], [800, 250]]).
      addSize([0, 0], [[300, 250], [320, 50]]).
      build();
      googletag.defineSlot('/1030735/glocktalk.com/thread_bottomleader', [[320, 50], [300, 250], [728, 90], [970, 250], [800, 250]], 'div-gpt-bottomleader').defineSizeMapping(mappingbottomleader).addService(googletag.pubads());
