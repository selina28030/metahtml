		<script>
			$(document).ready(function(){	
				var container =$("ul#slider");
				var total_slides =container.find('li').length;
				//console.log(total_slides);

				if($(".hm-phtsld").width() < 720) {
					jQuery('.hm-phtsld-in').lbSlider({
						infinite: false,
						leftBtn: '.hmsa-l',
						rightBtn: '.hmsa-r',
						visible: 1,
					});
				}else{
					jQuery('.hm-phtsld-in').lbSlider({
						infinite: false,
						leftBtn: '.hmsa-l',
						rightBtn: '.hmsa-r',
						visible: 1,
					});
				}

				var cont=0;

				page_disply(cont,total_slides);
			 
				$("div#slidebox a.hmsa-r").click(function(e){
					e.preventDefault();
					var $this=$(this);
					var dir=$this.data("dir");
					if(dir=='nxt'){
						cont=cont+1;
					}else{
						cont=cont-1;	 
					}
					if(cont <0 || cont>=total_slides){
						cont=0;
					}
					//console.log(cont);
					var containers =$("ul#slider");
					var total_slidesz =containers.find('li').length;		
					var total_slide=parseInt(total_slidesz-2);
					page_disply(cont,total_slide);
				});
				
				$("div#slidebox a.hmsa-l").click(function(e){
					e.preventDefault();
					var $this=$(this);
					var dir=$this.data("dir");
					if(dir=='prev'){
						cont=cont-1;
					}else{
						cont=cont+1;	 
					}
					if(cont <0 || cont>=total_slides){
						cont=total_slides-1;		
					}
					//console.log(cont);
					
					var containerleft =$("ul#slider");
					var total_slideslf =containerleft.find('li').length;		
					var total_slidelef=parseInt(total_slideslf-2);
					page_disply(cont,total_slidelef);
					//page_disply(cont);
				});	

				function page_disply(page,total_slides){
					var slidename=parseInt(page+1);
					var totalslide=parseInt(total_slides);
					if(slidename==0){slidename=1;}
					var shopage = slidename+"/"+totalslide ;
					document.getElementById("numslide").innerHTML=shopage;
				}

			});
		</script>

	</body>

