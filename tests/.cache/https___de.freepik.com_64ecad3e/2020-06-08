<aside id="header-filter" class="header-filter search-filter trigger" data-permanent="true">
    <div class="scrollable">
        <div class="row row--vertical-center row--space-between row--title pd-right-lv1">
            <h6 class="trigger__button button button--sm button--link-dark mg-none-i font-sm uppercase">Filter</h6>

            <button class="trigger__close button button--sm button--link-dark button--icon">
                <i class="icon--cross"></i>
            </button>
