<script>
	$(document).ready(function(){
		$(window).one('scroll',function() {
			load_podcast_widget();
			load_astrologyRashiHome_widget();
			load_astrologyPanchangHomeSide_widget();
		});
	});
	function load_podcast_widget(){
		$.ajax({
				url:'/load_homepage_podcast_ajax.html',
				type:'GET',
				cache:true,
				dataType : 'html',
				success:function(res){
					$("#podcast_div").html('');
					$("#podcast_div").html(res);
					if( $('.podcast-item').length ) {
						$('.podcast-item').slick({
							dots: false,
							infinite: true,
							speed: 500,
							slidesToShow: 5,
							slidesToScroll: 5,
							nextArrow: '.podcastRight',
							prevArrow: '.podcastLeft',
						});
					}
					
				},
				error:function(){
					console.log('podcast not loaded..');
				},
				 complete: function(){
					console.log("podcast loaded successfully");

				} 
			 });
	}
	function load_astrologyRashiHome_widget()
	{
		$.ajax({
				url:'/load_homepage_astrology_horoscope_ajax.html',
				type:'GET',
				cache:true,
				dataType : 'html',
				success:function(res){
					$("#RashiComponentHome_div").html('');
					$("#RashiComponentHome_div").html(res);
				},
				error:function(){
					console.log('Astrology home not loaded..');
				},
				 complete: function(){
					console.log("Astrology home loaded successfully");

				} 
			 });	
	}
	
	function load_astrologyPanchangHomeSide_widget()
	{
	$.ajax({
				url:'/load_homepage_astrology_sidebar_ajax.html',
				type:'GET',
				cache:true,
				dataType : 'html',
				success:function(res){
					$("#panchangHomeRhs_div").html('');
					$("#panchangHomeRhs_div").html(res);
				},
				error:function(){
					console.log('Astrology home sidebar not loaded..');
				},
				 complete: function(){
					console.log("Astrology home sidebar loaded successfully");

				} 
			 });	
	}
</script>


