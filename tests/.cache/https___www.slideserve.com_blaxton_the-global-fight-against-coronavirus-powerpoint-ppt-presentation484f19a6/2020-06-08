<script >
  var loadHandler ={};
  loadHandler.load = function(link,type,fun){
    var elmnt = document.createElement(type);
    if(type =="script")
    elmnt.src = link;
    else if(type =="link")
      elmnt.href = link;
  if(fun) elmnt.onload = fun;
  document.body.appendChild(elmnt);
  };
  //To load jquery
  function loadJquery() {
        loadHandler.load("/js/jquery.min.js","script",
       function(){
                loadHandler.load("/js/bootstrap.min.js","script",null);
        loadHandler.load("/js/main.js","script",null);
        if(typeof onjqueryload == 'function')
           onjqueryload();
           }
      );
      };
  var bLoadFirstTime = true;
  document.onmousemove = document.onscroll = document.onkeydown = document.onclick= function(e) {
    if(bLoadFirstTime){
      bLoadFirstTime = false;
      setTimeout(loadJquery,10);
    }
  };
  if(typeof bAutoLoadJquery !== "undefined" && bAutoLoadJquery == 'true'){
    window.addEventListener('load', (event) => {
      if(bLoadFirstTime){
        bLoadFirstTime = false;
        setTimeout(loadJquery,100);
      }
    });  
  }
</script>
