	<script>
		function cnnlive(){
			$('#cnn').addClass('active-select');
			$('#ibn7').removeClass('active-select');
			$.ajax({
				type: 'GET',
				url: 'https://www.news18.com//cnnlive-sidebartv.php',
				data:{type:'CNNIBN'},
				beforeSend: function() {
					// setting a timeout
					$('#livetvloader').show();
				},								
				success: function(posts){
					var item = $('#livetv').html(posts);
				},
				complete: function() {
					$('#livetvloader').hide();
				}
			});
		}

		function ibn7live(){
			$('#cnn').removeClass('active-select');
			$('#ibn7').addClass('active-select');
			$.ajax({
				type: 'GET',
				url: 'https://www.news18.com//ibn7live-sidebartv.php',
				data:{type:'IBN7'},	
				success: function(posts){
					var item = $('#livetv').html(posts);
				}
			});	
		}
	</script>
